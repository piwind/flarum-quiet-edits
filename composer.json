  ],
  "type": "flarum-extension",
  "license": "MIT",
  "authors": [{
    "name": "Hasan Ã–zbey",
    "email": "hasanoozbey@gmail.com",
    "homepage": "https://github.com/the-turk"
  }],
  "support": {
    "issues": "https://github.com/the-turk/flarum-quiet-edits/issues",
    "source": "https://github.com/the-turk/flarum-quiet-edits"
  },
  "require": {
    "flarum/core": "^1.0.2",
    "jfcherng/php-diff": "^6.10.0"
